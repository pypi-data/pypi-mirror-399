# Include important files
include LICENSE
include README.md
include CHANGELOG.md
include py.typed

# Include type stubs
recursive-include enkaliprime *.pyi
recursive-include enkaliprime py.typed

# Exclude development files
exclude .gitignore
exclude .env*
prune tests
prune examples
prune .git
prune .github
prune __pycache__
prune *.egg-info
prune build
prune dist
prune venv
prune .venv

