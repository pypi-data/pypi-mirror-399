{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Trace"
    }
  },
  "extensions": {
    "sqs": {
      "maxBatchSize": 10,
      "pollingIntervalInSeconds": 5,
      "visibilityTimeoutInSeconds": 30
    }
  },
  "functionTimeout": "00:05:00"
}
