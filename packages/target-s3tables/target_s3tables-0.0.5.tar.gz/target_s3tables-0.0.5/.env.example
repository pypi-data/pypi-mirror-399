# target-s3tables configuration
# Copy this file to .env and run with: target-s3tables --config=ENV

# Required
TARGET_S3TABLES_REGION=us-east-1

# Recommended: Glue Iceberg REST endpoint
TARGET_S3TABLES_CATALOG_MODE=glue_rest
TARGET_S3TABLES_ACCOUNT_ID=123456789012
TARGET_S3TABLES_TABLE_BUCKET_NAME=my-table-bucket

# Direct mode (alternative)
# TARGET_S3TABLES_CATALOG_MODE=s3tables_rest
# TARGET_S3TABLES_TABLE_BUCKET_ARN=arn:aws:s3tables:us-east-1:123456789012:bucket/my-table-bucket

# Optional
TARGET_S3TABLES_NAMESPACE=default
TARGET_S3TABLES_WRITE_MODE=append
TARGET_S3TABLES_BATCH_SIZE_ROWS=5000
TARGET_S3TABLES_SANITIZE_NAMES=true
TARGET_S3TABLES_CREATE_TABLES=true
TARGET_S3TABLES_EVOLVE_SCHEMA=true
