    print "Screen state: " + screenState;
}
print "✓ Screen with state";

print "\n=== Test 2: COMPONENT with props pattern ===";
component SmartButton {
    let label = "Click me";
    let enabled = true;
    print "Button: " + label;
}
print "✓ Component with props";

print "\n=== Test 3: THEME with color palette ===";
theme ModernTheme {
    let primary = "blue";
    let secondary = "gray";
    print "Theme colors: " + primary + ", " + secondary;
}
print "✓ Theme with palette";

print "\n=== Test 4: SCREEN with actions ===";
screen InteractiveScreen {
    action handleClick() {
        print "Clicked";
    }
    print "Screen ready";
}
print "✓ Screen with actions";

print "\n=== Test 5: COMPONENT composition pattern ===";
component List {
    print "List component";
}
component ListItem {
    print "List item";
}
print "✓ Component composition";

print "\n=== Test 6: THEME with dynamic values ===";
theme DynamicTheme {
    let mode = "dark";
    if (mode == "dark") {
        print "Dark mode active";
    }
}
print "✓ Dynamic theme";

print "\n=== Test 7: SCREEN with lifecycle hooks ===";
screen LifecycleScreen {
    action onMount() {
        print "Screen mounted";
    }
    action onUnmount() {
        print "Screen unmounted";
    }
    print "Lifecycle screen";
}
print "✓ Screen lifecycle";

print "\n=== Test 8: COMPONENT with events ===";
component EventButton {
    action onClick() {
        print "Button clicked";
    }
    action onHover() {
        print "Button hovered";
    }
    print "Event button ready";
}
print "✓ Component events";

print "\n=== Test 9: THEME inheritance ===";
