print "=== IF/ELSE TEST ==="

action check_number(n):
    if n > 10:
        return "Big number"
    else:
        return "Small number"

print check_number(15)
print check_number(5)

# Nested if
action grade_score(score):
    if score >= 90:
        return "A"
    else if score >= 80:
        return "B" 
    else:
        return "C"

print grade_score(85)
print grade_score(92)
print grade_score(75)
