// Test 1: While loop with break condition pattern
let searchVal = 3;
let found = false;
let idx = 0;
let list = [1, 2, 3, 4, 5];
while (idx < 5 && !found) {
    if (list[idx] == searchVal) {
        found = true;
        print "Found at index: " + idx;
    }
    idx = idx + 1;
}
// Expected: Found at index: 2

