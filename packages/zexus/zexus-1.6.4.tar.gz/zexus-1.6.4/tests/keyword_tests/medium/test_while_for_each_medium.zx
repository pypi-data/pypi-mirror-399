while (outer < 2) {
    let inner = 0;
    while (inner < 2) {
        print "(" + outer + ", " + inner + ")";
        inner = inner + 1;
    }
    outer = outer + 1;
}
// Expected: (0, 0), (0, 1), (1, 0), (1, 1)

