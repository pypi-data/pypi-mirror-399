# Installing Homebrew

    $ mkdir ~/Code; cd ~/Code; git clone git@gitlab.com:shortstory/print-server.git
    $ /bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install.sh)"

# Running the Print Server

    $ make run

