                        "type": "object",
                        "properties": {
                            "species": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "element": {
                                            "type": "string",
                                            "description": "Chemical element"
                                        },
                                        "occu": {
                                            "type": "number",
                                            "description": "Occupancy"
                                        }
                                    },
                                    "required": [
                                        "element",
                                        "occu"
                                    ]
                                }
                            },
                            "abc": {
                                "type": "array",
                                "items": {
                                    "type": "number"
                                },
                                "minItems": 3,
                                "maxItems": 3,
                                "description": "Fractional coordinates"
