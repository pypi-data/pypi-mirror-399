                },
                "number": {
                    "type": "integer",
                    "description": "Space group number"
                },
                "point_group": {
                    "type": "string",
                    "description": "Point group symbol"
                },
                "symprec": {
                    "type": "number",
                    "description": "Symmetry precision tolerance"
                },
                "angle_tolerance": {
                    "type": "number",
