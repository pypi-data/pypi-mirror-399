{
    "intel": {
        "configuration": {
            "cc": "mpicc -c",
            "codeCoverage": true,
            "f77": "mpif90",
            "f90": "mpif90",
            "fa2plk": "mpif90",
            "fpp": "-fpp",
            "ipo": false,
            "machine_opt": "-xCORE-AVX512 -mtune=skylake",
            "metis5_cc": "icc",
            "metis_cc": "icc",
            "ndimepar": false,
            "objects": "-module $O -c",
            "optimization": "-O3",
            "vector": 16
        },
        "modules": [
            "turbul",
            "solidz",
            "nastin",
            "alefor",
            "magnet",
            "partis",
            "levels",
            "fakemo",
            "chemic",
            "nastal",
            "temper",
            "exmedi"
        ]
    },
    "intel-i4": {
        "configuration": {
            "fa2p": "mpif90 -module ../../Utils/user/alya2pos -c -fpp",
            "libmetis": "-L../../Thirdparties/metis-4.0 -lmetis",
            "metis": "4"
        },
        "from": "intel",
        "precompilation": [
            "metis4",
            "alya2pos"
        ]
    },
    "intel-i4-debug": {
        "configuration": {
            "debug": "-ftrapuv -check all,noarg_temp_created -traceback -debug full -warn all,nodec,nointerfaces -fp-stack-check -ansi-alias"
        },
        "from": "intel-i4"
    },
    "intel-i4-openmp": {
        "configuration": {
            "openmp": "-qopenmp"
        },
        "from": "intel-i4"
    },
    "intel-i8": {
        "configuration": {
            "fa2p": "mpif90 -module ../../Utils/user/alya2pos -c -fpp -DI8",
            "integer": 8,
            "libmetis": "-L../../Thirdparties/metis-5.1.0_i8/build/Linux-x86_64/libmetis -lmetis",
            "metis": "5.1"
        },
        "from": "intel",
        "precompilation": [
            "metis5",
            "alya2pos"
        ]
    },
    "intel-i8-debug": {
        "configuration": {
            "debug": "-ftrapuv -check all,noarg_temp_created -traceback -debug full -warn all,nodec,nointerfaces -fp-stack-check -ansi-alias"
        },
        "from": "intel-i8"
    },
    "intel-i8-openmp": {
        "configuration": {
            "openmp": "-qopenmp"
        },
        "from": "intel-i8"
    }
}
