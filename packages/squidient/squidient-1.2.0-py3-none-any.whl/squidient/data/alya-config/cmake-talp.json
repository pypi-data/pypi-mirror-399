{
    "intel-i4-talp": {
        "configuration": {
            "DLB_DIR": "$DLB_HOME",
            "WITH_CODE_COVERAGE": false,
            "WITH_DLB_TALP": true
        },
        "file": "cmake-intel.json",
        "from": "intel-i4"
    },
    "intel-i8-talp": {
        "configuration": {
            "INTEGER_SIZE": 8
        },
        "from": "intel-i4-talp"
    },
    "intel-i4-talp-ipo": {
        "configuration": {
            "WITH_IPO": true
        },
        "from": "intel-i4-talp"
    },
    "intel-i4-talp-ndimepar": {
        "configuration": {
            "WITH_NDIMEPAR": true
        },
        "from": "intel-i4-talp"
    },
    "intel-i8-talp-ndimepar": {
        "configuration": {
            "INTEGER_SIZE": 8
        },
        "from": "intel-i4-talp-ndimepar"
    }
}
