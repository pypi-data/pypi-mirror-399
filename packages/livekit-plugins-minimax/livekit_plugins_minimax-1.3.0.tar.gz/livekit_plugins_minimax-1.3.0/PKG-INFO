    model="MiniMax-Text-01",     # æ¨¡å‹åç§°
    temperature=0.7,             # æ¸©åº¦
    max_tokens=2000,             # æœ€å¤§tokenæ•°
    system_prompt="ä½ æ˜¯ä¸€ä¸ªæœ‰å¸®åŠ©çš„AIåŠ©æ‰‹"  # ç³»ç»Ÿæç¤ºè¯
)
