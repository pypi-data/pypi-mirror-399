    model="speech-01-turbo",     # æ¨¡å‹åç§°
    voice_id="female-tianmei",   # è¯­éŸ³ID
    speed=1.0,                   # è¯­é€Ÿ (0.5-2.0)
    vol=1.0,                     # éŸ³é‡ (0.0-2.0)
    pitch=0,                     # éŸ³è°ƒ (-12åˆ°12)
    emotion="neutral"            # æƒ…æ„Ÿ (neutral, happy, sad, angry)
)

# è‡ªå®šä¹‰LLMé…ç½®
