  "operators": {
    "+": {
      "symbol": "+",
      "precedence": 10,
      "associativity": "left",
      "enabled": true
    },
    "-": {
      "symbol": "-",
      "precedence": 10,
      "associativity": "left",
      "enabled": true
    },
    "*": {
      "symbol": "*",
      "precedence": 20,
      "associativity": "left",
      "enabled": true
    },
    "/": {
      "symbol": "/",
      "precedence": 20,
      "associativity": "left",
      "enabled": true
    },
    "==": {
      "symbol": "==",
      "precedence": 5,
      "associativity": "none",
      "enabled": true
    },
    "!=": {
      "symbol": "!=",
      "precedence": 5,
      "associativity": "none",
      "enabled": true
    },
    ">": {
      "symbol": ">",
      "precedence": 5,
      "associativity": "none",
      "enabled": true
    },
    "<": {
      "symbol": "<",
      "precedence": 5,
      "associativity": "none",
      "enabled": true
    },
    ">=": {
      "symbol": ">=",
      "precedence": 5,
      "associativity": "none",
      "enabled": true
    },
    "<=": {
      "symbol": "<=",
      "precedence": 5,
      "associativity": "none",
      "enabled": true
    },
    "=": {
      "symbol": "=",
      "precedence": 1,
      "associativity": "right",
      "enabled": true
    }
