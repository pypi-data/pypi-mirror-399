  "functions": {
    "print": {
      "name": "print",
      "arity": -1,
      "implementation": "builtin.print",
      "description": "Print to stdout",
      "enabled": true
    },
    "Number": {
      "name": "Number",
      "arity": 1,
      "implementation": "builtin.to_number",
      "description": "Convert to number",
      "enabled": true
    },
    "String": {
      "name": "String",
      "arity": 1,
      "implementation": "builtin.to_string",
      "description": "Convert to string",
      "enabled": true
    },
    "Boolean": {
      "name": "Boolean",
      "arity": 1,
      "implementation": "builtin.to_boolean",
      "description": "Convert to boolean",
      "enabled": true
    },
    "List": {
      "name": "List",
      "arity": -1,
      "implementation": "builtin.list",
      "description": "Create list",
      "enabled": true
    }
